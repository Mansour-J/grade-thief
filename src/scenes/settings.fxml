<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.image.Image?>
<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <CheckBox alignment="TOP_LEFT" layoutX="444.0" layoutY="338.0" mnemonicParsing="false" text="Sound">
         <font>
            <Font name="Avenir Next Regular" size="24.0" />
         </font>
      </CheckBox>
      <Slider layoutX="420.0" layoutY="490" maxWidth="200.0" prefWidth="200.0" />
      <Text fill="#3091ff" layoutX="361.0" layoutY="200.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Settings">
         <font>
            <Font size="72.0" />
         </font>
      </Text>
      <Text fontSmoothingType="LCD" layoutX="456.0" layoutY="463.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sensitivity">
         <font>
            <Font name="Avenir Next Regular" size="24.0" />
         </font>
      </Text>

      <fx:define>
         <Image fx:id="backImage" url="/res/images/back_img.png" />
      </fx:define>

      <Button layoutX="135.0" layoutY="135.0" mnemonicParsing="false" onAction="#backClickedFromSettings" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView image="$backImage"/>
         </graphic>
      </Button>
   </children>
</AnchorPane>
